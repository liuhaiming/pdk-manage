<mxDefaultToolbar>
	<!-- <add as="save" action="saveGraph" icon="images/save.gif"/> -->
	<add as="print" action="print" icon="static/global/plugins/mxgraph/images/print.gif"/>
	<add as="ci" action="ciGraph" icon="static/global/plugins/mxgraph/images/commit.png"/>
	<add as="exportImage" action="exportImage" icon="static/global/plugins/mxgraph/images/image.gif"/>
	<!--<add as="copyFlow" action="copyFlow" icon="static/global/plugins/mxgraph/images/copy.gif"/>-->
	<br/><hr />
	<add as="select" mode="select" icon="static/global/plugins/mxgraph/images/select.gif"/>
	<add as="pan" mode="pan" icon="static/global/plugins/mxgraph/images/pan.gif"/>
	<add as="connect" mode="connect" icon="static/global/plugins/mxgraph/images/connect.gif"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge != null)
			{
				editor.defaultEdge.style = 'verticalEdge';
			}
		}
	]]></add>
	<add as="connect" mode="connect" icon="static/global/plugins/mxgraph/images/straight.gif"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge != null)
			{
				editor.defaultEdge.style = 'straightEdge';
			}
		}
	]]></add>
	<add as="connect" mode="connect" icon="static/global/plugins/mxgraph/images/zx.png"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge != null)
			{
				editor.defaultEdge.style = 'orthogonalEdge';
			}
		}
	]]></add>
	<br/><hr/>
	<add as="undo" action="undo" icon="static/global/plugins/mxgraph/images/undo.gif"/>
	<add as="redo" action="redo" icon="static/global/plugins/mxgraph/images/redo.gif"/>
	<br/><hr class="toolbar" />
	<add as="cut" action="cut" icon="static/global/plugins/mxgraph/images/cut.gif"/>
	<add as="copy" action="copy" icon="static/global/plugins/mxgraph/images/copy.gif"/>
	<add as="paste" action="paste" icon="static/global/plugins/mxgraph/images/paste.gif"/>
	<add as="delete" action="delete" icon="static/global/plugins/mxgraph/images/delete.gif"/>
	<br/><hr class="toolbar" />
	<!-- 
	<add as="group" action="group" icon="images/group.gif"/>
	<add as="ungroup" action="ungroup" icon="images/ungroup.gif"/>
	<br/><hr/> -->
	<!-- 
	<add as="Swimlane" template="swimlane" icon="images/swimlane.gif"/>-->
	<add as="Task" template="task" icon="static/global/plugins/mxgraph/images/rectangle.gif"/>
	
	<!-- 
	<add as="Subprocess" template="subprocess" icon="images/rounded.gif"/>
	<add as="Ellipse" template="shape" style="ellipse" icon="images/ellipse.gif"/>
	-->
	<add as="Rhombus" template="condt" style="rhombus" icon="static/global/plugins/mxgraph/images/rhombus.gif"/>
	<add as="Ellipse" template="shape" style="ellipse" icon="static/global/plugins/mxgraph/images/ellipse.gif"/>
	<add as="RoundTask" template="round" style="ellipse" icon="static/global/plugins/mxgraph/images/round.gif"/>
	<!-- 
	<add as="Actor" template="shape" style="actor" icon="images/actor.gif"/>-->
	<br/><hr/>
	
	<add as="fit" action="fit" icon="static/global/plugins/mxgraph/images/zoom.gif"/>
	<add as="zoomIn" action="zoomIn" icon="static/global/plugins/mxgraph/images/zoomin.gif"/>
	<add as="zoomOut" action="zoomOut" icon="static/global/plugins/mxgraph/images/zoomout.gif"/>
	<add as="actualSize" action="actualSize" icon="static/global/plugins/mxgraph/images/zoomactual.gif"/>
	<add as="zoom" action="zoom" icon="static/global/plugins/mxgraph/images/zoom.gif"/>
	<br/><hr/>
	<!-- 
	<add as="outline" action="toggleOutline" icon="images/outline.gif"/>-->
	<add as="tasks" action="toggleTasks" icon="static/global/plugins/mxgraph/images/tasks.gif"/>
	<add as="xml" action="viewXml" icon="static/global/plugins/mxgraph/images/xml.gif"/>
	<!-- <add as="layout" action="autoLayout" icon="images/layout.png"/>
	<add as="help" action="toggleHelp" icon="images/help.gif"/>
	<add as="console" action="toggleConsole" icon="images/console.gif"/>-->
